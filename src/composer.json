{
    "name": "nhida/levart-test",
    "description": "REST API to send an email (Levart Test)",
    "require": {
        "php" : ">=8",
        "squizlabs/php_codesniffer": "^3.7",
        "phpmailer/phpmailer": "^6.8",
        "league/oauth2-server": "^8.4",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.0",
        "guzzlehttp/psr7": "^1.9",
        "php-amqplib/php-amqplib": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*"
    },
    "autoload": {
        "psr-4": {
            "Nhida\\LevartTest\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nhida\\LevartTest\\": "tests"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "authors": [
        {
            "name": "nurinhidayatullah",
            "email": "n.hidayatullah94@gmail.com"
        }
    ]
